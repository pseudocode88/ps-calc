{
  "name": "trader-bruh",
  "version": "3.0",
  "description": "Position Size Calculator",
  "main": "index.js",
  "keywords": [
    "trade",
    "position size",
    "calculator"
  ],
  "scripts": {
    "start": "electron .",
    "build": "electron-forge package",
    "ship": "node build.js"
  },
  "author": "pseudocode88",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.7.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "electron": "^25.2.0"
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "Trader bruh",
            "icon": "assets/logo.icns"
          }
        }
      ],
      "packagerConfig": {
        "icon": "assets/logo.icns",
        "name": "Trader Bruh",
        "iconSize": "10"
      },
      "ignore": [
        ".gitignore",
        "package.json",
        "build.js",
        "README.md",
        "yarn.lock",
        ".fleet/"
      ]
    }
  }
}